<Window x:Class="SJBCS.GUI.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:student="clr-namespace:SJBCS.GUI.Student"
        xmlns:local="clr-namespace:SJBCS.GUI"
        mc:Ignorable="d"
        Title="TestWindow" Height="300" Width="300">
    <Window.DataContext>
        <local:TestWindowViewModel />
    </Window.DataContext>
    <Window.Resources>
        <DataTemplate DataType="{x:Type student:EnrollBiometricsViewModel}">
            <student:EnrollBiometricsView />
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl Content="Model" />
    </Grid>
</Window>
